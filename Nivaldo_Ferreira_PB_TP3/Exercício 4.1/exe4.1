import ipaddress

def verificar_ip_no_prefixo(ip, prefixo):
    try:
        rede = ipaddress.ip_network(prefixo, strict=False)
        ip_obj = ipaddress.ip_address(ip)
        return ip_obj in rede
    except ValueError:
        return False

ip = "192.168.1.5"
prefixo = "192.168.1.0/24"
if verificar_ip_no_prefixo(ip, prefixo):
    print(f"O IP {ip} está dentro do prefixo {prefixo}.")
else:
    print(f"O IP {ip} não está dentro do prefixo {prefixo}.")
