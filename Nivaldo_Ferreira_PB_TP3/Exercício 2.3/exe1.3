import math
import multiprocessing

def is_prime(n):
    if n <= 1:
        return False
    for i in range(2, int(math.sqrt(n)) + 1):
        if n % i == 0:
            return False
    return True

def count_primes_in_range(start, end):
    count = 0
    for i in range(start, end):
        if is_prime(i):
            count += 1
    return count

def parallel_prime_count(n):
    cores = multiprocessing.cpu_count()
    pool = multiprocessing.Pool(cores)
    step = n // cores
    ranges = [(i * step, (i + 1) * step) for i in range(cores)]
    
    ranges[-1] = (ranges[-1][0], n)
    
    result = pool.starmap(count_primes_in_range, ranges)
    pool.close()
    pool.join()
    
    return sum(result)

if __name__ == "__main__":
    n = 100000
    total_primes = parallel_prime_count(n)
    print(f'NÃºmero de primos entre 1 e {n}: {total_primes}')