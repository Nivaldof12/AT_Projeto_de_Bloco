from multiprocessing import Pool
import random

def sum_chunk(chunk):
    return sum(chunk)

data = [random.randint(1, 100000) for _ in range(10000)]
chunks = [data[i:i + 1000] for i in range(0, len(data), 1000)]

with Pool() as pool:
    result = sum(pool.map(sum_chunk, chunks))
print(f"Soma total: {result}")