class Estudante:
    def __init__(self, nome, nota):
        self.nome = nome
        self.nota = nota
    
    def __repr__(self):
        return f"{self.nome}: {self.nota}"

def quicksort(lista):
    if len(lista) <= 1:
        return lista
    
    pivo = lista[0]
    menores = [x for x in lista if x.nota < pivo.nota]
    iguais = [x for x in lista if x.nota == pivo.nota]
    maiores = [x for x in lista if x.nota > pivo.nota]
    
    return quicksort(menores) + iguais + quicksort(maiores)

estudantes = [
    Estudante("Ana", 8.5),
    Estudante("Carlos", 7.0),
    Estudante("Bruno", 9.2),
    Estudante("Mariana", 6.8),
    Estudante("Pedro", 7.5)
]

resultado = quicksort(estudantes)

for e in resultado:
    print(e)
