def permutacoes(string, atual=""):
    if len(string) == 0:
        print(atual)
        return
    
    usado = set()
    for i in range(len(string)):
        if string[i] not in usado:
            usado.add(string[i])
            restante = string[:i] + string[i+1:]
            permutacoes(restante, atual + string[i])

entrada = "aba"
permutacoes(entrada)
